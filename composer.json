{
  "name": "igorkgg/flysystem-akamai-netstorage",
  "description": "Akamai Netstorage filesystem adapter for Flysystem.",
  "keywords": [
    "flysystem",
    "filesystem",
    "akamai",
    "netstorage",
    "file",
    "files"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Akamai\\Open\\EdgeGrid\\": "edgegrid/",
      "League\\Flysystem\\AkamaiNetStorage\\": "netstorage/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Akamai\\Open\\EdgeGrid\\Tests\\": "tests/edgegrid/",
        "League\\Flysystem\\AkamaiNetStorage\\Tests\\": "tests/netstorage/"
    }
  },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.5.0",
    "monolog/monolog": "^3.0",
    "ext-fileinfo": "*",
    "league/flysystem": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpspec/prophecy-phpunit": "^2.0.x-dev"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Kosteski",
      "email": "igor.kosteski@gmail.com"
    }
  ]
}
